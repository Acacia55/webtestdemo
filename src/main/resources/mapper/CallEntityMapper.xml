  <select id="orderCallByName" parameterType="java.lang.String" resultType="com.example.webtestdemo.dao.entity.CallEntity">
    SELECT *
    FROM call
    WHERE userid =#{userid,jdbcType=VARCHAR}
    ORDER BY name asc
  </select>

  <insert id="insert" parameterType="com.example.webtestdemo.dao.entity.CallEntity">
    insert into call (userid, name, phonenumber)
    values (#{userid,jdbcType=VARCHAR}, #{name,jdbcType=VARCHAR},
            #{phonenumber,jdbcType=VARCHAR})
  </insert>

  <delete id="deletebyphone" parameterType="com.example.webtestdemo.dao.entity.CallEntity">
    delete from date
    where phonenumber = #{phonenumber,jdbcType=VARCHAR}
